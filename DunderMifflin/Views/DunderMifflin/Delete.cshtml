@model ToDo

<p>Name: @Model.Name</p> 
<p>Description: @Model.Description</p>
<p>Due Date: @Model.DueDate</p>
<p>Completed: @Model.Completed</p>


<p>Do you want to delete this task?</p>

<form asp-action="Delete" asp-controller="DunderMifflin" method="post">
    <input hidden name="Id" value="@Model.Id" />
    <input type="submit" value="Yes" />

</form>

<br>

<a asp-action="MyPage" asp-controller="DunderMifflin"><button>No</button></a>